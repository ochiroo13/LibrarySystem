<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="view.MemberController">
	<children>
		<ImageView fitHeight="150.0" fitWidth="200.0"
			layoutY="327.0" pickOnBounds="true" preserveRatio="true">
			<image>
				<Image url="@../../Resourses/JustLogo.png" />
			</image>
		</ImageView>
		<TextField fx:id="txtFirstNameMember" layoutX="527.0"
			layoutY="53.0" onAction="#textChangedMember" prefHeight="31.0"
			prefWidth="274.0" />
		<Label alignment="BOTTOM_LEFT" layoutX="433.0" layoutY="58.0"
			prefHeight="21.0" prefWidth="94.0" text="First name:"
			textFill="#13a6ff" />
		<Label alignment="BOTTOM_LEFT" layoutX="433.0" layoutY="97.0"
			prefHeight="21.0" prefWidth="94.0" text="Last name:"
			textFill="#13a6ff" />
		<TextField fx:id="txtLastNameMember" layoutX="527.0"
			layoutY="92.0" prefHeight="31.0" prefWidth="274.0" />
		<TextField fx:id="txtEmailMember" layoutX="527.0"
			layoutY="169.0" prefHeight="31.0" prefWidth="274.0" />
		<Label alignment="BOTTOM_LEFT" layoutX="433.0" layoutY="174.0"
			prefHeight="21.0" prefWidth="94.0" text="Email" textFill="#13a6ff" />
		<Label alignment="BOTTOM_LEFT" layoutX="433.0" layoutY="135.0"
			prefHeight="21.0" prefWidth="94.0" text="Phone:" textFill="#13a6ff" />
		<TextField fx:id="txtPhoneMember" layoutX="527.0"
			layoutY="130.0" prefHeight="31.0" prefWidth="274.0" />
		<Label alignment="BOTTOM_LEFT" layoutX="433.0" layoutY="253.0"
			prefHeight="21.0" prefWidth="94.0" text="Gender" textFill="#13a6ff" />
		<Label alignment="BOTTOM_LEFT" layoutX="433.0" layoutY="214.0"
			prefHeight="21.0" prefWidth="94.0" text="Birth date"
			textFill="#13a6ff" />
		<Button fx:id="btnCancelMember" defaultButton="true"
			layoutX="697.0" layoutY="295.0" mnemonicParsing="false"
			onAction="#clickCancelMember" prefHeight="31.0" prefWidth="106.0"
			text="Cancel" textAlignment="CENTER" textFill="#13a6ff"
			underline="true" />
		<Button fx:id="btnSaveMember" defaultButton="true"
			layoutX="579.0" layoutY="295.0" mnemonicParsing="false"
			onAction="#clickSaveMember" prefHeight="31.0" prefWidth="106.0"
			text="Save" textAlignment="CENTER" textFill="#13a6ff" />
		<TableView fx:id="tblMember" layoutX="8.0" layoutY="53.0"
			prefHeight="270.0" prefWidth="416.0">
			<columnResizePolicy>
				<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
			</columnResizePolicy>
		</TableView>
		<ComboBox fx:id="cboGenderMember" layoutX="527.0"
			layoutY="248.0" prefHeight="31.0" prefWidth="276.0"
			visibleRowCount="2" />
		<Button fx:id="btnAddMember" defaultButton="true" layoutX="8.0"
			layoutY="14.0" mnemonicParsing="false" onAction="#clickAddMember"
			prefHeight="31.0" prefWidth="106.0" text="Add" textAlignment="CENTER"
			textFill="#13a6ff" />
		<DatePicker fx:id="datBirthDateMember" layoutX="527.0"
			layoutY="209.0" prefHeight="31.0" prefWidth="275.0" />
	</children>
</AnchorPane>
